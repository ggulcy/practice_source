<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androiduipractice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".UI.Button.ButtonActivity" />
        <activity android:name=".UI.EditText.EditTextActivity" />
        <activity android:name=".UI.Recycler.ListActivity" />
        <activity android:name=".UI.TabViewPager.TabViewPagerActivity" />
        <activity
            android:name=".UI.Menu.MenuActivity"
            android:theme="@style/NoActionTheme" />
    </application>



</manifest>